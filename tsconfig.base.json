{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"declaration": true,
		"declarationMap": true,
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"lib": ["ESNext", "DOM"]
	}
}
