{
	"name": "walletnext",
	"description": "Wallet Next SDK is a implementation of the Webmax protocol for Dapp and Wallet",
	"version": "0.0.2",
	"keywords": [
		"wallet",
		"dapp",
		"web3",
		"web-wallet",
		"ethereum"
	],
	"author": "Aoi Kimura <aoi.kimura@intmax.io>",
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"main": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npm run clean && pnpm tsx ./build.ts",
		"build:watch": "npm run clean && pnpm tsx ./build.ts --watch",
		"clean": "rimraf dist",
		"typecheck": "tsc --noEmit"
	},
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js"
		},
		"./wallet": {
			"types": "./dist/types/wallet/index.d.ts",
			"import": "./dist/esm/wallet/index.js"
		},
		"./dapp": {
			"types": "./dist/types/dapp/index.d.ts",
			"import": "./dist/esm/dapp/index.js"
		},
		"./wagmi-legacy": {
			"types": "./dist/types/plugins/wagmi-v1/index.d.ts",
			"import": "./dist/esm/plugins/wagmi-v1/index.js"
		},
		"./rainbowkit-legacy": {
			"types": "./dist/types/plugins/rainbowkit-v1/index.d.ts",
			"import": "./dist/esm/plugins/rainbowkit-v1/index.js"
		},
		"./wagmi": {
			"types": "./dist/types/plugins/wagmi-v2/index.d.ts",
			"import": "./dist/esm/plugins/wagmi-v2/index.js"
		},
		"./rainbowkit": {
			"types": "./dist/types/plugins/rainbowkit-v2/index.d.ts",
			"import": "./dist/esm/plugins/rainbowkit-v2/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"types": [
				"./dist/types/index.d.ts"
			],
			"wallet": [
				"./dist/types/wallet/index.d.ts"
			],
			"dapp": [
				"./dist/types/dapp/index.d.ts"
			],
			"wagmi-legacy": [
				"./dist/types/plugins/wagmi-v1/index.d.ts"
			],
			"rainbowkit-legacy": [
				"./dist/types/plugins/rainbowkit-v1/index.d.ts"
			],
			"wagmi": [
				"./dist/types/plugins/wagmi-v2/index.d.ts"
			],
			"rainbowkit": [
				"./dist/types/plugins/rainbowkit-v2/index.d.ts"
			]
		}
	},
	"peerDependencies": {
		"rainbowkit-v2": "npm:@rainbow-me/rainbowkit@2.x",
		"wagmi-v1": "npm:wagmi@1.x",
		"wagmi-v2": "npm:wagmi@2.x"
	},
	"peerDependenciesMeta": {
		"wagmi-v1": {
			"optional": true
		},
		"wagmi-v2": {
			"optional": true
		},
		"rainbowkit-v2": {
			"optional": true
		}
	},
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^20.11.16",
		"arg": "^5.0.2",
		"esbuild": "^0.20.0",
		"esbuild-svelte": "^0.8.0",
		"glob": "^10.3.10",
		"rainbowkit-v2": "npm:@rainbow-me/rainbowkit@^2.0.1",
		"svelte": "^4.2.11",
		"svelte-check": "^3.6.4",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.6.2",
		"tsx": "^4.7.1",
		"viem": "^2.7.15",
		"wagmi-v1": "npm:wagmi@^1.4.13",
		"wagmi-v2": "npm:wagmi@^2.5.7"
	},
	"dependencies": {
		"eventemitter3": "^5.0.1"
	}
}
