{
	"name": "@webmax/core",
	"description": "A simplest protocol that way to wallet connect for dapp and web wallet",
	"version": "0.0.1",
	"keywords": ["wallet", "dapp", "web3", "web-wallet", "ethereum"],
	"author": "",
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"main": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npm run clean && npm run build:esm",
		"build:esm": "tsc --project tsconfig.json",
		"clean": "rimraf dist",
		"typecheck": "tsc --noEmit"
	},
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js"
		},
		"./wallet": {
			"types": "./dist/types/wallet/index.d.ts",
			"import": "./dist/esm/wallet/index.js"
		},
		"./dapp": {
			"types": "./dist/types/dapp/index.d.ts",
			"import": "./dist/esm/dapp/index.js"
		},
		"./types/": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"types": ["./dist/types/index.d.ts"],
			"wallet": ["./dist/types/wallet/index.d.ts"],
			"dapp": ["./dist/types/dapp/index.d.ts"]
		}
	},
	"devDependencies": {
		"viem": "^2.7.1"
	},
	"dependencies": {
		"eventemitter3": "^5.0.1"
	}
}
